class TreeNode {
    int value = 0;
    TreeNode left = null;
    TreeNode right = null;
    
    void setValue(int v) {
        value = v;
    }
    
    int getValue() {
        return value;
    }
    
    void setLeft(TreeNode n) {
        left = n;
    }
    
    void setRight(TreeNode n) {
        right = n;
    }
    
    TreeNode getLeft() {
        return left;
    }
    
    TreeNode getRight() {
        return right;
    }
    
    boolean hasLeft() {
        return left != null;
    }
    
    boolean hasRight() {
        return right != null;
    }
    
    boolean isLeaf() {
        return left == null && right == null;
    }
}

class BinaryTree {
    TreeNode root = null;
    
    void insert(int val) {
        TreeNode newNode = new TreeNode();
        newNode.setValue(val);
        
        if (root == null) {
            root = newNode;
        } else {
            insertNode(root, newNode);
        }
    }
    
    void insertNode(TreeNode current, TreeNode newNode) {
        if (newNode.getValue() < current.getValue()) {
            if (current.hasLeft()) {
                insertNode(current.getLeft(), newNode);
            } else {
                current.setLeft(newNode);
            }
        } else {
            if (current.hasRight()) {
                insertNode(current.getRight(), newNode);
            } else {
                current.setRight(newNode);
            }
        }
    }
    
    void printInOrder() {
        printInOrderNode(root);
        println();
    }
    
    void printInOrderNode(TreeNode node) {
        if (node != null) {
            printInOrderNode(node.getLeft());
            print(node.getValue());
            print(" ");
            printInOrderNode(node.getRight());
        }
    }
    
    int sumAll() {
        return sumNode(root);
    }
    
    int sumNode(TreeNode node) {
        if (node == null) {
            return 0;
        }
        return node.getValue() + sumNode(node.getLeft()) + sumNode(node.getRight());
    }
    
    int countNodes() {
        return countNode(root);
    }
    
    int countNode(TreeNode node) {
        if (node == null) {
            return 0;
        }
        return 1 + countNode(node.getLeft()) + countNode(node.getRight());
    }
    
    boolean contains(int val) {
        return containsNode(root, val);
    }
    
    boolean containsNode(TreeNode node, int val) {
        if (node == null) {
            return false;
        }
        if (node.getValue() == val) {
            return true;
        }
        if (val < node.getValue()) {
            return containsNode(node.getLeft(), val);
        }
        return containsNode(node.getRight(), val);
    }
}

