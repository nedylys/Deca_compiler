class GrandParent {
    void qui() {
        println("GrandParent");
    }
}

class ParentX extends GrandParent {
    void qui() {
        println("ParentX");
    }
}

class EnfantX extends ParentX {
    void qui() {
        println("EnfantX");
    }
}

{
    GrandParent g = new GrandParent();
    ParentX p = new ParentX();
    EnfantX e = new EnfantX();
    
    g.qui();
    p.qui();
    e.qui();
}
