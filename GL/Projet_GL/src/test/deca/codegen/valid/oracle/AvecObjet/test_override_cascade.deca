// Description:
//    Test de polymorphisme et override en cascade.
//    Classes A, B, C, D, E avec override de la méthode ret().
//    E hérite de D sans override (donc utilise ret() de D).
//
// Resultats:
//    Pas d'erreur. Affiche : 0 1 2 3 3
//    (a.ret()=0, b.ret()=1, c.ret()=2, d.ret()=3, e.ret()=3)

class A {
    int ret() {
        return 0 ;
    }
}


class B extends A {
    int ret() {
        return 1 ;
    }
}

class C extends A {
    int ret() {
        return 2 ;
    }
}

class D extends A {
    int ret() {
        return 3 ;
    }
}

class E extends D {

}

// on s'attend à 0 1 2 3 3

{
    A a = new A() ;
    A b = new B() ;
    A c = new C() ;
    A d = new D() ;
    A e = new E() ;
    println(a.ret()) ;
    println(b.ret()) ;
    println(c.ret()) ;
    println(d.ret()) ;
    println(e.ret()) ;
}