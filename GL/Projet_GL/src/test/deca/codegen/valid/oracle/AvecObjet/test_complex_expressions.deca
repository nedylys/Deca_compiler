class Helper {
    int compute(int a, int b) {
        return a * 2 + b;
    }
    
    boolean check(int x) {
        return x > 0 && x < 100;
    }
    
    int getValue() {
        return 42;
    }
}

{
    Helper h = new Helper();
    
    // Expressions arithmétiques complexes
    int a = 5;
    int b = 3;
    int c = 2;
    
    int r1 = a + b * c - a / b + c % a;
    int r2 = (a + b) * (c - a) / (b + c);
    int r3 = -a + (-b) * (-c);
    int r4 = h.compute(a, b) + h.compute(b, c);
    int r5 = h.compute(h.compute(1, 2), h.compute(3, 4));
    boolean b1 = a > 0 && b > 0 || c < 0;
    boolean b2 = !(a == b) && (b != c || a == c);
    boolean b3 = h.check(a) && h.check(b) && !h.check(-1);
    float f1 = (float)(a) / (float)(b);
    int i1 = (int)(f1 * 10.0);
    int max = a;

    println(r1);
    

    println(r2);
    
    
    println(r3);
    
    // Expressions avec appels de méthodes
    
    println(r4);
    
    
    println(r5);
    
    // Expressions booléennes complexes

    
    if (b1) { print(1); } else { print(0); }
    if (b2) { print(1); } else { print(0); }
    if (b3) { print(1); } else { print(0); }
    println();
    
    // Conditions complexes dans if
    if (a > 0 && (b < 10 || c == 2) && h.check(a + b)) {
        println(1);
    } else {
        println(0);
    }
    
    // Expressions avec cast

    println(i1);
    
    // Expression ternaire simulée

    if (b > max) { max = b; }
    if (c > max) { max = c; }
    println(max);
}
