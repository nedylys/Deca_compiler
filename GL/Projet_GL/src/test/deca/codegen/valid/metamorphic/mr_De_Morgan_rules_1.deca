// Description:
//    Test métamorphique : lois de De Morgan (version 1).
//    Doit produire le même résultat que mr_De_Morgan_rules_2.deca
//    en utilisant !(t && f) et !(t || f).
//
// Resultats:
//    Pas d'erreur. Affiche : ok, ok
//    (Identique à mr_De_Morgan_rules_2.deca)

{
    boolean t = true ;
    boolean f = false ; 

    boolean c_1 = !(t && f) ;
    boolean c_2 = !(t || f) ;

    if (c_1) {
        println("ok") ;
    }
    else {
        println("pas ok") ;
    }

    if (c_2) {
        println("pas ok") ;
    }
    else {
        println("ok") ;
    }
}